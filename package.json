{
  "name": "npc-cli",
  "version": "1.0.0",
  "private": true,
  "description": "npc-cli",
  "author": "robmyers",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop -p 8011",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "postinstall": "patch-package",
    "types": "tsc --noEmit",
    "ts-script": "NODE_OPTIONS=\"--no-experimental-fetch\" ts-node -r tsconfig-paths/register -O '{ \"module\": \"commonjs\", \"isolatedModules\": false }'",
    "symbols-meta": "yarn ts-script src/scripts/symbols-meta",
    "watch-symbols-meta": "nodemon --delay 2s --config src/scripts/symbols-meta.nodemon.json --exec 'yarn symbols-meta'"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@giscus/react": "^2.4.0",
    "@loadable/component": "^5.16.3",
    "@mdx-js/react": "^2.3.0",
    "@react-three/drei": "^9.99.5",
    "@react-three/fiber": "^8.15.16",
    "@react-three/offscreen": "0.0.8",
    "@react-three/rapier": "^1.3.0",
    "@tanstack/react-query": "^5.17.19",
    "braces": "^3.0.2",
    "cli-columns": "^4.0.0",
    "debounce": "^2.0.0",
    "ecctrl": "^1.0.66",
    "flexlayout-react": "^0.7.15",
    "gatsby": "^5.13.1",
    "gatsby-plugin-emotion": "^8.13.0",
    "gatsby-plugin-fontawesome": "^1.3.1",
    "gatsby-plugin-mdx": "^5.13.0",
    "gatsby-source-filesystem": "^5.13.0",
    "getopts": "^2.3.0",
    "htmlparser2": "^9.1.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "mvdan-sh": "^0.5.0",
    "pixi.js": "^7.4.0",
    "poly2tri": "^1.5.0",
    "polygon-clipping": "^0.15.7",
    "react": "^18.2.0",
    "react-beforeunload": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-pro-sidebar": "^1.1.0-alpha.2",
    "react-use-measure": "^2.1.1",
    "safe-stable-stringify": "^2.3.1",
    "svg-path-parser": "^1.1.0",
    "three": "^0.162.0",
    "uid": "^2.0.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-webgl": "^0.17.0-beta.36",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-generator-functions": "^7.23.9",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@types/braces": "^3.0.4",
    "@types/loadable__component": "^5.13.8",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-beforeunload": "^2.1.5",
    "@types/react-dom": "^18.2.18",
    "@types/svg-path-parser": "^1.1.6",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "babel-preset-gatsby": "^3.13.1",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "patch-package": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
